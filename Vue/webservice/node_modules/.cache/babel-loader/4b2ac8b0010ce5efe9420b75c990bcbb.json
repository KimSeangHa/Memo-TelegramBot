BA,qBAkBA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,4CAAA,EAAA;AACA,QAAA,YAAA,EAAA,KAAA;AADA,OAAA,EAGA,IAHA,CAGA,UAAA,MAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA;;AACA,YAAA,MAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,cAAA,MAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,cAAA,EAAA;AAAA,cAAA,WAAA,EAAA,KAAA,CAAA;AAAA,aAAA;AACA,WAFA,MAEA,IAAA,MAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,YAAA,KAAA,CAAA,4BAAA,CAAA;AACA,WAFA,MAEA;AACA,YAAA,KAAA,CAAA,aAAA,CAAA;AACA;AACA,SARA,MAQA;AACA,UAAA,KAAA,CAAA,0BAAA,CAAA;AACA;AACA,OAhBA,EAiBA,KAjBA,CAiBA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,uBAAA,CAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OApBA;AAqBA,KAxCA;AAyCA,IAAA,SAzCA,uBAyCA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,WAAA,OAAA,GAAA,KAAA,OAAA,GAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA;AACA;AA9CA,GARA;AAwDA,EAAA,QAAA,EAAA;AACA,IAAA,YAAA,EAAA,wBAAA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,UAAA;AACA;AAHA;AAxDA,CAAA","sourcesContent":["<template>\r\n    <div >\r\n        <p class=\"MemoAdd-text\"><img :src=\"require(`@/assets/images/memo_icon.png`)\" class=\"memoImg\" />Add Memo</p>\r\n        <p class=\"MemoAdd-text telegram\" @click=\"Makebot\"><img :src=\"require(`@/assets/images/telegram.jpg`)\" class=\"memoImg\" />Make Bot</p>\r\n        <div class=\"memo-form\">\r\n            <!-- <ckeditor :editor=\"editor\" v-model=\"content\"></ckeditor> -->\r\n            <textarea \r\n                v-model=\"content\" \r\n                class=\"memo-content\" \r\n                placeholder=\"내용을 입력해주세요.\"\r\n                @keydown.enter.exact.prevent\r\n                @keyup.enter.exact=\"enterFunc\"\r\n                @keydown.enter.shift.exact=\"enterFunc\"\r\n                :maxlength=\"contentMax\"\r\n            >\r\n            </textarea>\r\n            <div style=\"width:100%; text-align:right;\">\r\n                <div class=\"limitText\" >500글자 이하로 작성해주세요.</div>\r\n                <div class=\"limitTextView\" >{{ contentMax }} / <span style=\"color:red\">{{ contentMax - content.length }}</span></div>\r\n            </div>\r\n            <br>\r\n            <div style=\"text-align: right;\">\r\n                <button button class=\"addMemoBtn\" @click=\"addMemo\">등록</button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'MemoForm',\r\n    data: function () {\r\n        return {\r\n            content: '',\r\n            contentMax: 500\r\n        }\r\n    },\r\n    methods: {\r\n        reset () {\r\n            this.content = ''\r\n        },\r\n        addMemo () { \r\n            if ( content === '') {\r\n                alert(\"내용을 입력해주세요.\")\r\n                return false\r\n            }\r\n            \r\n            const { content, member_token } = this\r\n            const id = new Date().getTime()\r\n            const regDate = this.$moment().format('YYYY-MM-DD HH:mm:ss')\r\n            const curDate = this.$moment().format('YYYY-MM-DD')\r\n            console.log(curDate);\r\n            this.$store.commit('memo/addMemo', { content, id, regDate, curDate, member_token })\r\n            this.reset()\r\n        },\r\n        Makebot () {\r\n            axios.post('http://192.168.0.5:9090/webservice/Makebot', { \r\n                member_token: this.member_token\r\n            })\r\n            .then((result) => {\r\n                console.log(result)\r\n                if (result.status === 200) {\r\n                    if(result.data === 100) {\r\n                        this.$store.commit('login/Logout', { MemberToken: this.memberToken })\r\n                    } else if (result.data === 1) {\r\n                        alert(\"권한이 없습니다. 관리자를 통하여 실행해주세요.\")\r\n                    } else {\r\n                        alert(\"Bot 생성 완료 !\")\r\n                    }\r\n                } else {\r\n                    alert(\"Bot 생성 실패. 관리자에게 문의해주세요.\")\r\n                }\r\n            })\r\n            .catch(e => {\r\n                alert(\"Error : 관리자에게 문의해주세요.\")\r\n                console.log(e)\r\n            }) \r\n        },\r\n        enterFunc () {\r\n            console.log('aaa')\r\n            const enter = '\\n'\r\n            this.content = this.content + enter\r\n            console.log(this.content)\r\n        }\r\n    },\r\n    computed: {\r\n        member_token: function() {\r\n            return this.$store.state.login.loginToken\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.memo-form {\r\n    border: 1px solid #ccc;\r\n    border-left: none;\r\n    border-right: none;\r\n    border-top: none;\r\n    padding: 20px;\r\n    margin: 20px;\r\n    padding-bottom: 40px;\r\n    padding-top: 0px;\r\n    position: relative;\r\n}\r\n\r\n.memoImg {\r\n    width: 30px;\r\n    height: 30px;\r\n    margin-right: 5px;\r\n    margin-bottom: 3px;\r\n}\r\n\r\n.MemoAdd-text {\r\n    font-size: 18px;\r\n    margin-left: 40px;\r\n    margin-top: 30px;\r\n    display: inline-block;\r\n}\r\n\r\n.telegram {\r\n    float: right;\r\n    margin-right: 40px;\r\n    cursor: pointer;\r\n}\r\n.memo-content {\r\n    width: 100%;\r\n    height: 100px;\r\n}\r\n\r\n.addMemoBtn {\r\n    width: 80px;\r\n    height: 40px;\r\n    margin-top: 20px;\r\n    position: relative;\r\n    border: 1px solid #ddd;\r\n    background-color : #fff;\r\n    border-radius: 8px;\r\n    font-size: 14px;\r\n}\r\n\r\n.addMemoBtn:hover {\r\n    background-color : #172d58;\r\n    color:#fff;\r\n}\r\n.limitText {\r\n    display:inline-block;\r\n    font-size:12px;\r\n    color:#666;\r\n    width: 200px;\r\n    margin-right: 20px;\r\n}\r\n\r\n.limitTextView {\r\n    display:inline-block;\r\n    font-size:12px;\r\n    color:#666;\r\n}\r\n</style>"],"sourceRoot":"src/components/memo"}]}