<mapper namespace="login">

	<insert id="InsertMember" parameterType="com.spring.webservice.vo.loginVO">
		INSERT 
		  INTO MEMBER ( MEMBER_NAME
		            , MEMBER_ID
		            , MEMBER_PASSWORD
		            , MEMBER_REGDATE
		            )
		     VALUES ( #{member_name}
		            , #{member_id}
		            , #{member_password}
		            , #{member_regdate}
		            )
 	</insert>
 </mapper>